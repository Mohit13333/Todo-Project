{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/register",
        "dest": "/server.js"
      },
      {
        "src": "/login",
        "dest": "/server.js"
      },
      {
        "src": "/user",
        "dest": "/server.js"
      },
      {
        "src": "/contact",
        "dest": "/server.js"
      },
      {
        "src": "/post",
        "dest": "/server.js"
      },
      {
        "src": "/getTodos",
        "dest": "/server.js"
      },
      {
        "src": "/deleteTodos/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/updateTodos/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/toggleCheck/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/getSortedTodos",
        "dest": "/server.js"
      }
    ]
  }
  